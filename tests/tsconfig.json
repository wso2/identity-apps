{
    "compilerOptions": {
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "jsx": "react",
        "resolveJsonModule": true,
        "noLib": false,
        "suppressImplicitAnyIndexErrors": true,
        "baseUrl": ".",
        "declaration": true,
        "experimentalDecorators": true,
        "lib": [
            "es6",
            "dom"
        ],
        "outDir": "dist",
        "paths": {
            "*": ["types/*"],
            "@wso2/identity-cypress-test-base/api": ["node_modules/@wso2/identity-cypress-test-base/dist/api/commands"],
            "@wso2/identity-cypress-test-base/ui": ["node_modules/@wso2/identity-cypress-test-base/dist/ui"]
        },
        "target": "es5",
        "types": [
            "cypress",
            "node"
        ],
        "sourceMap": false
    },
    "include": [
        "integration",
        "fixtures",
        "plugins",
        "support"
    ],
    "compileOnSave": false,
    "exclude": [
        "node_modules",
        "./output",
        "*.test.js",
        "*.test.jsx",
        "*.test.ts",
        "*.test.tsx",
        "*.spec.js",
        "*.spec.jsx",
        "*.spec.ts",
        "*.spec.tsx"
    ]
}
