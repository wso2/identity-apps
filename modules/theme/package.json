{
    "private": true,
    "name": "@wso2is/theme",
    "version": "2.0.70",
    "description": "Theme for web components of WSO2 Identity Server Webapps.",
    "keywords": [
        "WSO2",
        "WSO2 IS",
        "WSO2 Identity Server",
        "WSO2 Identity Apps",
        "theme"
    ],
    "main": "src/index.js",
    "scripts": {
        "prebuild": "pnpm clean:build",
        "build": "pnpm prebuild && pnpm compile -- --skipSample --skipManifest",
        "build:dev": "pnpm build -- --skipHashing",
        "clean": "pnpm clean:lock-files && pnpm clean:build && pnpm clean:node-modules",
        "clean:build": "pnpm rimraf dist",
        "clean:lock-files": "pnpm rimraf package-lock.json && pnpm rimraf pnpm-lock.yaml && pnpm rimraf yarn.lock",
        "clean:node-modules": "pnpm rimraf node_modules",
        "compile": "node scripts/build.js",
        "lint": "pnpm lint:all",
        "lint:all": "pnpm lint:targeted -- .",
        "lint:autofix": "pnpm lint:all -- --fix",
        "lint:targeted": "eslint --ext .js,.jsx,.ts,.tsx --resolve-plugins-relative-to . --max-warnings=0",
        "test": "pnpm jest",
        "test:watch": "pnpm test -- --watch",
        "jest": "pnpm jest --passWithNoTests",
        "watch": "watch \"pnpm build\" src"
    },
    "author": "WSO2",
    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/core": "^7.11.4",
        "@babel/register": "^7.8.6",
        "@types/jest": "^26.0.14",
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.32.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "clean-css": "^4.2.1",
        "eslint": "^7.20.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-react": "^7.18.3",
        "eslint-plugin-react-hooks": "^4.0.0",
        "fs-extra": "^9.0.1",
        "jest": "^26.4.2",
        "less": "^3.10.3",
        "less-plugin-rewrite-variable": "0.1.0",
        "less-to-json": "^1.2.0",
        "merge-files": "^0.1.2",
        "rc-tree": "^4.0.0-beta.2",
        "replace": "^1.1.5",
        "rimraf": "^3.0.2",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-less": "^2.4.1",
        "ts-jest": "^26.4.0",
        "ts-node": "^10.8.1",
        "watch": "^1.0.2"
    }
}
