        "@typescript-eslint/eslint-plugin": "^2.19.2",
        "@typescript-eslint/parser": "^2.19.2",
        "cypress": "^5.2.0",
        "eslint": "^7.4.0",
        "eslint-plugin-cypress": "^2.11.1",
        "eslint-plugin-import": "^2.20.2",
        "rimraf": "^3.0.2",
        "run-script-os": "^1.0.7",
        "typescript": "^4.0.2"
