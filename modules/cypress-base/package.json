{
    "name": "@wso2is/cypress-base",
    "version": "1.0.430",
    "description": "Commonly used commands, configs, utilities for cypress integration tests for WSO2 Identity Apps",
    "author": "WSO2",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "WSO2",
        "WSO2 IS",
        "WSO2 Identity Server",
        "WSO2 Identity Apps",
        "integration-tests",
