        "@testing-library/react": "^11.0.4",
        "@types/jest": "^26.0.14",
        "@types/lodash-es": "^4.17.4",
        "babel-polyfill": "^6.26.0",
        "jest": "^26.4.2",
        "rimraf": "^3.0.2",
        "run-script-os": "^1.0.7",
        "ts-jest": "^26.4.0"
    },
    "peerDependencies": {
