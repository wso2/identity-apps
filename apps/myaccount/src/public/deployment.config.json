{
    "consoleApp": {
        "path": "/console/develop/getting-started"
    },
    "appBaseName": "",
    "appBaseNameForHistoryAPI": "/",
    "clientID": "MY_ACCOUNT",
    "debug": false,
    "extensions": {
        "applicationInsightsEnabled": false,
        "applicationInsightsCookieDomain": "",
