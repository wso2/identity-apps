{
    "__experimental__platformIdP": {
        "enabled": false,
        "homeRealmId": ""
    },
    "appBaseName": "myaccount",
    "clientID": "MY_ACCOUNT",
    "consoleApp": {
        "origin": "https://localhost:9000",
        "path": "/getting-started"
    },
    "idpConfigs": {
        "authorizeEndpointURL": "${serverOrigin}/oauth2/authorize",
        "clockTolerance": 300,
        "enableOIDCSessionManagement": false,
        "logoutEndpointURL": "${serverOrigin}/oidc/logout",
        "oidcSessionIFrameEndpointURL": "${serverOrigin}/oidc/checksession",
        "responseMode": "query",
        "serverOrigin": "https://localhost:9443"
    },
    "isSaas": false,
    "loginCallbackPath": "",
    "logoutCallbackPath": "",
    "proxyContextPath": "",
    "routePaths": {
        "home": "/overview",
        "login": "",
        "logout": "/logout"
    },
    "serverOrigin": "https://localhost:9443",
    "session": {
        "checkSessionInterval": 3.0,
        "sessionRefreshTimeOut": 300.0,
        "userIdleTimeOut": 600.0,
        "userIdleWarningTimeOut": 580.0
    },
    "superTenantProxy": "",
    "tenantContext": {
        "enableTenantQualifiedUrls": true,
        "requireSuperTenantInAppUrls": true,
        "requireSuperTenantInUrls": false
    },
    "ui": {
        "announcements": [],
        "appCopyright": "${copyright} ${year} WSO2 LLC.",
        "appLogoPath": "/assets/images/branding/logo.svg",
        "appMobileLogoPath": "/assets/images/branding/logo-mini.svg",
        "appName": "My Account",
        "appTitle": "My Account | WSO2 Identity Server",
        "appWhiteLogoPath": "/assets/images/branding/logo-inverted.svg",
        "authenticatorApp": {
            "enabled": true
        },
        "cookiePolicyUrl": "/authenticationendpoint/cookie_policy.do",
        "disableMFAForFederatedUsers": true,
        "enableMFAUserWise": true,
        "features": {
            "applications": {
                "disabledFeatures": [],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "delete": [],
                    "read": [],
                    "update": []
                }
            },
            "approvals": {
                "disabledFeatures": [],
                "enabled": true,
                "featureFlags": [],
                "scopes": {
                    "read": [
                        "internal_approval_task_view"
                    ],
                    "update": [
                        "internal_approval_task_update"
                    ]
                }
            },
            "operations": {
                "disabledFeatures": [],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "delete": [],
                    "read": [
                        "internal_humantask_view"
                    ],
                    "update": []
                }
            },
            "overview": {
                "disabledFeatures": [],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "delete": [],
                    "read": [],
                    "update": []
                }
            },
            "personalInfo": {
                "disabledFeatures": [ "profileInfo.profile.legacy" ],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "delete": [],
                    "read": [],
                    "update": []
                }
            },
            "security": {
                "disabledFeatures": [
                    "security.accountRecovery.challengeQuestions",
                    "security.accountRecovery.legacyRecoveryMethodStatusCheck",
                    "security.loginVerifyData.typingDNA",
                    "security.mfa.sms"
                ],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "delete": [],
                    "read": [],
                    "update": []
                }
            }
        },
        "forceBackupCode": true,
        "i18nConfigs": {
            "langAutoDetectEnabled": true,
            "showLanguageSwitcher": true
        },
        "isCookieConsentBannerEnabled": true,
        "isHeaderAvatarLabelAllowed": true,
        "isMultipleEmailsAndMobileNumbersEnabled": true,
        "isPasswordInputValidationEnabled": true,
        "isProfileUsernameReadonly": true,
        "privacyPolicyConfigs": {},
        "productName": "WSO2 Identity Server",
        "productVersionConfig": {
            "versionOverride": "7.2.0"
        },
        "showAppSwitchButton": "true",
        "theme": {
            "name": "wso2is"
        }
    }
}
