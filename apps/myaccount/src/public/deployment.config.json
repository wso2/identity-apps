{
    "consoleApp": {
        "path": "/getting-started",
        "origin": "https://localhost:9000"
    },
    "isSaas": false,
    "appBaseName": "myaccount",
    "clientID": "MY_ACCOUNT",
    "loginCallbackPath": "",
    "logoutCallbackPath": "",
    "serverOrigin": "https://localhost:9443",
    "routePaths": {
        "home": "/overview",
        "login": "",
        "logout": "/logout"
    },
    "extensions": {
        "applicationInsightsCookieDomain": "",
        "applicationInsightsCookiePostfix": "",
        "applicationInsightsEnabled": "false",
        "applicationInsightsInstrumentationKey": "",
        "applicationInsightsProxyEndpoint": ""
    },
    "superTenantProxy": "",
    "idpConfigs": {
        "logoutEndpointURL": "${serverOrigin}/oidc/logout",
        "clockTolerance": 300,
        "authorizeEndpointURL": "${serverOrigin}/oauth2/authorize",
        "oidcSessionIFrameEndpointURL": "${serverOrigin}/oidc/checksession",
        "serverOrigin": "https://localhost:9443",
        "responseMode": "query",
        "enableOIDCSessionManagement": false
    },
    "session": {
        "userIdleWarningTimeOut": 580.0,
        "sessionRefreshTimeOut": 300.0,
        "userIdleTimeOut": 600.0,
        "checkSessionInterval": 3.0
    },
    "legacyAuthzRuntime": false,
    "ui": {
        "announcements": [],
        "appCopyright": "${copyright} ${year} WSO2 LLC.",
        "appTitle": "My Account | WSO2 Identity Server",
        "appName": "My Account",
        "appLogoPath": "/assets/images/branding/logo.svg",
        "appWhiteLogoPath": "/assets/images/branding/logo-white.svg",
        "enableMFAUserWise": true,
        "disableMFAForFederatedUsers": true,
        "forceBackupCode": true,
        "showAppSwitchButton": "true",
        "authenticatorApp": {
            "enabled": true
        },
        "features": {
            "applications": {
                "disabledFeatures": [],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "read": [],
                    "update": [],
                    "delete": []
                }
            },
            "operations": {
                "disabledFeatures": [],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "read": ["internal_humantask_view"],
                    "update": [],
                    "delete": []
                }
            },
            "overview": {
                "disabledFeatures": [],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "read": [],
                    "update": [],
                    "delete": []
                }
            },
            "personalInfo": {
                "disabledFeatures": [
                    "profileInfo.mobileVerification"
                ],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "read": [],
                    "update": [],
                    "delete": []
                }
            },
            "security": {
                "disabledFeatures": ["security.loginVerifyData.typingDNA"],
                "enabled": true,
                "scopes": {
                    "create": [],
                    "read": [],
                    "update": [],
                    "delete": []
                }
            }
        },
        "isCookieConsentBannerEnabled": true,
        "isPasswordInputValidationEnabled": true,
        "cookiePolicyUrl": "/authenticationendpoint/cookie_policy.do",
        "isHeaderAvatarLabelAllowed": true,
        "i18nConfigs": {
            "showLanguageSwitcher": true,
            "langAutoDetectEnabled": true
        },
        "privacyPolicyConfigs": {},
        "isProfileUsernameReadonly": true,
        "productName": "WSO2 Identity Server",
        "productVersionConfig": {
            "versionOverride": ""
        },
        "theme": {
            "name": "wso2is"
        }
    }
}
