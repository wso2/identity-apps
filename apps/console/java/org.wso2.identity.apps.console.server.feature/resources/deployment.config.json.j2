                                {% endif %}
                            {% endfor %}
                        }{{ "," if not loop.last }}
                    {% else %}
                        "{{ level_1_key }}": "{{ level_1_value }}"{{ "," if not loop.last }}
                    {% endif %}
                {% endfor %}
            ]{{ "," if not loop.last }}
        {% else %}
            "{{ key }}": "{{ value }}"{{ "," if not loop.last }}
        {% endif %}
