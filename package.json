{
    "private": true,
    "name": "@wso2is/webapps",
    "version": "1.2.887",
    "description": "WSO2 Identity Server Web Applications",
    "keywords": [
        "WSO2",
        "WSO2 IS",
        "WSO2 Identity Server",
        "WSO2 Identity Apps"
    ],
    "repository": "https://github.com/wso2/identity-apps.git",
    "scripts": {
        "bootstrap": "pnpm install",
        "bootstrap:hoist": "pnpm run bootstrap",
        "build:dev": "pnpm run bootstrap && pnpm run build -- --filter=!@wso2is/theme && turbo run build:dev --filter=@wso2is/theme",
        "build": "turbo run build",
        "build:apps": "turbo run build --filter=@wso2is/myaccount --filter=@wso2is/console",
        "build:modules": "turbo run build --filter=./modules/*",
        "build:external": "turbo run build --filter=!@wso2is/console --filter=!@wso2is/myaccount && turbo run build:external --filter=@wso2is/console --filter=@wso2is/myaccount",
        "build:external:static": "turbo run build --filter=!@wso2is/console --filter=!@wso2is/myaccount && turbo run build:external:static --filter=!@wso2is/console --filter=!@wso2is/myaccount",
        "build:theme": "turbo run build --filter=@wso2is/theme && turbo run copy:themes:src --filter=@wso2is/myaccount --filter=@wso2is/console",
        "clean": "turbo run clean",
        "clean-all": "pnpm run remove-package-lock && pnpm run remove-maven-folders && pnpm run remove-node-modules",
        "nyc:full-report": "pnpx nyc report --reporter=lcov --reporter=text",
        "nyc:text-report": "pnpx nyc report --reporter=text",
        "nyc:text-summary-report": "pnpx nyc report --reporter=text-summary",
        "remove-node-modules": "npx lerna exec -- rimraf -rf node_modules && rimraf -rf node_modules",
        "remove-package-lock": "npx lerna exec -- rimraf -rf package-lock.json && rimraf -rf package-lock.json",
        "remove-maven-folders": "npx lerna exec -- rimraf -rf target npm ./apps/myaccount/src/main ./apps/console/src/main && rimraf -rf target npm ./apps/user-portal/src/main ./apps/console/src/main",
        "test": "pnpm run test:unit",
        "test:watch": "pnpm run test -- --watch",
        "test:integration": "turbo run test --filter=@wso2is/integration-tests",
        "test:integration:smoke": "turbo run test:smoke --filter=@wso2is/integration-tests",
        "test:integration:interactive": "turbo run test:interactive --filter=@wso2is/integration-tests",
        "test:unit": "turbo run test --filter=!@wso2is/integration-tests -filter=!@wso2is/form",
        "test:unit:coverage": "pnpm run test:unit -- -- -- --coverage --collect-coverage",
        "test:unit:coverage:aggregate": "node scripts/aggregate-unit-test-coverage.js",
        "test:unit:coverage:aggregated": "pnpm run test:unit:coverage && pnpm run test:unit:coverage:aggregate",
        "test:unit:apps": "turbo run test --filter=@wso2is/console --filter=@wso2is/myaccount",
        "test:unit:console": "turbo test --filter=@wso2is/console",
        "test:unit:core": "turbo test --filter=@wso2is/core",
        "test:unit:form": "turbo test --filter=@wso2is/form",
        "test:unit:forms": "turbo test --filter=@wso2is/forms",
        "test:unit:i18n": "turbo test --filter=@wso2is/i18n",
        "test:unit:modules": "turbo test --filter=./modules/*",
        "test:unit:myaccount": "turbo test  --filter=@wso2is/myaccount",
        "test:unit:react-components": "turbo test --filter=@wso2is/react-components",
        "test:unit:theme": "turbo test --filter=@wso2is/theme",
        "test:unit:validation": "turbo test --filter=@wso2is/validation",
        "update-version": "node scripts/update-version.js",
        "lint": "turbo lint",
        "lint:autofix": "pnpm run lint:autofix",
        "lint:ci": "turbo lint:ci",
        "lint:staged": "eslint -c .eslintrc.js $(git diff --diff-filter=d --name-only | grep -E \"(.js$|.ts$|.jsx$|.tsx$)\")",
        "typecheck": "turbo typecheck"
    },
    "author": "WSO2",
    "license": "Apache-2.0",
    "dependencies": {
        "@asgardeo/auth-react": "^1.1.2",
